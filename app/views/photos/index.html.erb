

	<%= render 'layouts/menubar' %>

<ul class='thumbnails thumb scroll'>


	<% @routes.each do |r| %>
	 <% if r.image.present? %>
		<li class='span4'>
			<%= link_to r.image_url, class: :thumbnail do %>
				<%= image_tag r.image_url, class: :imagetag %>
			<% end %>
		 </li>
		<% end %>
	<% end %> 
</ul>

<div class='row '>
			<div class='span12'>
				<div class='back-my-settings'>
				
					<%= link_to '<< Back To User', user_path(current_user), class:"btn btn-large btn-success" %>
			</div>
		</div>
	</div>
	
<h6 class='logophotos'><em>Biker's Trip</em></h6>

